{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers": "Origin, X-Requested-With, Content-Type, Accept",
      "maxAge": 86400
    }
  ]
}
